@model DodavanjePopravnogIspitaVM


<br />
<br />
<br />


<form asp-controller="PopravniIspit" asp-action="Dodaj" method="post">
    <input asp-for="SkolskaGodinaId" hidden readonly />
    <input asp-for="SkolaId" hidden readonly />
    <input asp-for="PredmetId" hidden readonly />
    <label>1. Član komisije:</label>
    <select asp-for="ClanId1" asp-items="@Model.Clan"></select>
    <br />
    <label>2. Član komisije:</label>
    <select asp-for="ClanId2" asp-items="@Model.Clan2"></select>
    <br />
    <label>3. Član komisije:</label>
    <select asp-for="ClanId3" asp-items="@Model.Clan3"></select>
    <br />
    <label>Datum ispita:</label>
    <input asp-for="Datum" />
    <br />
    <label>Škola:</label>
    <input asp-for="Skola" readonly />
    <br />
    <label>Školska godina:</label>
    <input asp-for="SkolskaGodina" readonly />
    <br />
    <label>Predmet:</label>
    <input asp-for="Predmet" readonly />
    <br />
    <input type="submit" value="Snimi" class="btn btn-default" />
</form>